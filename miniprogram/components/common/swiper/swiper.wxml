<!--components/common/swiper.wxml-->
<swiper autoplay="{{autoplay}}" indicator-dots="{{dots}}" interval="{{interval}}" class="swiper">
  <block wx:for="{{list}}"  wx:key="index">
    <swiper-item class="swiper-item">
    <image src="{{item.image || item}}" mode="widthFix"></image>
  </swiper-item>
  </block>
</swiper>